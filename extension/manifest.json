{
  "manifest_version": 3,
  "name": "Gowtham's Extension",
  "version": "1.0",
  "description": "Automate Python scripts execution",
  "permissions": [
    "activeTab",
    "scripting",
    "background" 
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "host_permissions": [
    "http://127.0.0.1:5000/"  
  ]
}
